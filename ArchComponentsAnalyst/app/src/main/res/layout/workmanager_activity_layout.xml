<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.auto.aacanalyst.workmanager.WorkManagerActivity">

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/acb_workmanager"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="10dp"
        android:text="OneTimeWorkRequest"
        android:textColor="@android:color/white"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textSize="16sp"
        android:gravity="center"
        android:background="@android:color/holo_blue_light"
        android:onClick="onClickOneTimeRequest"
        app:layout_constraintTop_toTopOf="parent"/>

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/acb_room"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="10dp"
        android:text="PeriodicWorkRequest"
        android:textColor="@android:color/white"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textSize="16sp"
        android:gravity="center"
        android:background="@android:color/holo_blue_light"
        android:onClick="onClickPeriodicRequest"
        app:layout_constraintTop_toBottomOf="@id/acb_workmanager"/>

    <!--<android.support.v7.widget.AppCompatButton-->
        <!--android:id="@+id/acb_paging"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="48dp"-->
        <!--android:layout_marginTop="10dp"-->
        <!--android:text="FetchAvailableStatue"-->
        <!--android:textColor="@android:color/white"-->
        <!--android:textAllCaps="false"-->
        <!--android:textStyle="bold"-->
        <!--android:textSize="16sp"-->
        <!--android:gravity="center"-->
        <!--android:background="@android:color/holo_blue_light"-->
        <!--android:onClick="onClickFetchAvailableStatue"-->
        <!--app:layout_constraintTop_toBottomOf="@id/acb_room"/>-->

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_request_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/acb_room">

    </android.support.v7.widget.RecyclerView>

</android.support.constraint.ConstraintLayout>
